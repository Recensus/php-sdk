{
    "name": "recensus/php-sdk",
    "description": "The Recensus PHP SDK",
    "autoload": {
        "classmap": ["src/"]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Recensus/zend_http_client.git"
        }
    ],
    "require": {   
        "php": ">5.1.0",
        "recensus/zend-http-client": "dev-master"
    },
    "require-dev": {
        "php": ">5.3.3",
        "symfony/finder": "2.1.*",
        "symfony/class-loader": "2.1.*",
        "phpunit/phpunit": "3.7.9"
    }
}
